<% provide(:header, "Partida #{@game.name} - Desafios") %>
<% provide(:title, "Partida #{@game.name}|Desafios") %>

<div class="panel panel-default">
  <div class="panel-heading clearfix">
    <h4 class="pull-left">Desafios</h4>
    <%= link_to "Adicionar um desafio", new_profile_game_history_challenge_path(@game, @history),
        class: "btn btn-primary pull-right" %>
  </div>

  <div class="panel-body">
    <%= render 'table' %>
  </div>
</div>
