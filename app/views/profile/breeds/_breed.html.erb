<tr>
  <td><%= link_to breed.name, [:profile, @game, breed] %></td>
  <td>
    <%= link_to 'Editar', edit_profile_game_breed_path(@game, breed),
                        class: 'btn btn-primary' %>
    <%= link_to 'Deletar', [:profile, @game, breed],
                method: :delete, data: { confirm: 'Tem certeza?' },
                class: 'btn btn-danger' %>
  </td>
</tr>
