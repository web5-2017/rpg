<tr>
  <td><%= link_to breed.name, [:profile, @history, breed] %></td>
  <td>
    <%= link_to 'Editar', edit_profile_history_breed_path(@history, breed),
                        class: 'btn btn-primary' %>
    <%= link_to 'Deletar', [:profile, @history, breed],
                method: :delete, data: { confirm: 'Tem certeza?' },
                class: 'btn btn-danger' %>
  </td>
</tr>
