<tr>
  <td><%= user.name %></td>
  <td><%= l user.created_at, format: :short %></td>
  <td>
    <% unless current_user.friend_list.exists? user.id %>
      <%= link_to 'Adicionar', profile_user_adding_friend_path(user, search: {query: @query}),
                               id: "user_#{user.id}" %>
    <% end %>
  </td>
</tr>
