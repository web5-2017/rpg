<header class="page-header text-center clearfix">
  <h3>Personagens</h3>
  <div class="pull-right">
    <%= link_to 'Novo Personagem', new_match_history_character_path(@history), class: 'btn btn-primary' %>
  </div>
</header>

<div class="row">
  <div class="col-md-9 col-md-push-1">
    <table id="table-histories" class="table table-hover">
      <thead>
        <tr>
          <th>Nome</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <%= render @characters %>
      </tbody>
    </table>
  </div>
</div>
